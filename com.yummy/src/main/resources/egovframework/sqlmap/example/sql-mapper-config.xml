<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC
  "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- MyBatis configLocation Property >> context-mapper.xml -->
<!-- MyBatis에서 자바 타입에 대한 짧은 별칭을 지정 해 주는 것. -->
<configuration>
  <typeAliases>
    <typeAlias alias="egovMap"
      type="egovframework.rte.psl.dataaccess.util.EgovMap" />
    <typeAlias alias="searchVO"
      type="egovframework.example.sample.service.SampleDefaultVO" />
    <typeAlias alias="sampleVO"
      type="egovframework.example.sample.service.SampleVO" />

    <!-- 각 package에 있는 vo 파일과 매치시킴. -->
    <typeAlias alias="memb" type="egovframework.memb.vo.Memb" />
    <typeAlias alias="rst" type="egovframework.rst.vo.Rst" />
    <typeAlias alias="catag" type="egovframework.catag.vo.Catag"/>
    <typeAlias alias="rvw" type="egovframework.rvw.vo.Rvw"/>
    <typeAlias alias="rater" type="egovframework.rater.vo.Rater"/>
    <typeAlias alias="phot" type="egovframework.phot.vo.Phot"/>
    <typeAlias alias="menu" type="egovframework.menu.vo.Menu"/>
  </typeAliases>
</configuration>